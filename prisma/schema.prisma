generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model greetings {
  id         Int       @id(map: "Greeting_pkey") @default(autoincrement()) @db.SmallInt
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  text       String?   @default("")
}

model smoothies {
  id         Int      @id @default(autoincrement()) @db.SmallInt
  created_at DateTime @default(now()) @db.Timestamptz(6)
  title      String
  method     String
  rating     Int      @db.SmallInt
}
